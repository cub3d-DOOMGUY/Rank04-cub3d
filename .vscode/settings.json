{
  "[c]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "files.associations": {
    "**/*.pug": "jade",
    "**/*.h": "c",
    "**/*.c": "c",
    "**/*.cpp": "cpp",
    "**/*.html": "html",
    "**/templates/**/*.html": "django-html",
    "**/templates/**/*.css": "css",
    "**/requirements{/**,*}.{txt,in}": "pip-requirements",
    "*.html": "html",
    "*.xpm": "c",
    "*.h": "c",
    "*.hpp": "cpp",
    "*.njk": "nunjucks",
    "**/templates/**/*": "django-txt",
    "*.pug": "jade",
    "*.list": "properties",
    "*.sav": "json",
    "*.lalrpop": "rust",
    "**/*.mk": "makefile",
    "ios": "c",
    "algorithm": "c",
    "chrono": "c",
    "limits": "c",
    "ratio": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "atomic": "c",
    "istream": "c",
    "locale": "c",
    "string": "c",
    "system_error": "c",
    "unordered_map": "c",
    "vector": "c",
    "__locale": "c"
  }
}
